{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "anhskohbo/no-captcha": "^2.3",
        "appstract/laravel-multisite": "^0.2.0",
        "arcanedev/log-viewer": "^4.3",
